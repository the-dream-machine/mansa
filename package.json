{
	"name": "fishcake",
	"version": "0.0.0",
	"license": "MIT",
	"bin": "dist/cli.js",
	"type": "module",
	"engines": {
		"node": ">=16"
	},
	"scripts": {
		"postinstall": "patch-package",
		"build": "tsc",
		"dev": "tsc --watch",
		"test": "prettier --check . && xo && ava"
	},
	"files": [
		"dist"
	],
	"dependencies": {
		"@inkjs/ui": "^1.0.0",
		"@tanstack/react-query": "^5.0.5",
		"execa": "^8.0.1",
		"figures": "^5.0.0",
		"fuse.js": "^7.0.0",
		"ink": "^4.1.0",
		"ink-tab": "^5.0.0",
		"ink-task-list": "^2.0.0",
		"pastel": "^2.0.0",
		"patch-package": "^8.0.0",
		"react": "^18.2.0",
		"shelljs": "^0.8.5",
		"string-width": "^6.1.0",
		"zod": "^3.21.4",
		"zx": "^7.2.3"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^3.0.1",
		"@types/ink-divider": "^2.0.3",
		"@types/react": "^18.0.32",
		"@typescript-eslint/eslint-plugin": "^6.9.0",
		"@typescript-eslint/parser": "^6.9.0",
		"@vdemedes/prettier-config": "^2.0.1",
		"ava": "^5.2.0",
		"bun-types": "latest",
		"chalk": "^5.2.0",
		"eslint": "^8.0.1",
		"eslint-config-semistandard": "^17.0.0",
		"eslint-config-standard": "^17.1.0",
		"eslint-config-xo-react": "^0.27.0",
		"eslint-plugin-import": "^2.25.2",
		"eslint-plugin-n": "^15.0.0 || ^16.0.0 ",
		"eslint-plugin-promise": "^6.0.0",
		"eslint-plugin-react": "^7.32.2",
		"eslint-plugin-react-hooks": "^4.6.0",
		"ink-testing-library": "^3.0.0",
		"prettier": "^2.8.7",
		"ts-node": "^10.9.1",
		"typescript": "^5.0.3",
		"xo": "^0.54.2"
	},
	"ava": {
		"extensions": {
			"ts": "module",
			"tsx": "module"
		},
		"nodeArguments": [
			"--loader=ts-node/esm"
		]
	},
	"xo": {
		"extends": "xo-react",
		"prettier": true,
		"rules": {
			"react/prop-types": "off"
		}
	},
	"prettier": "@vdemedes/prettier-config",
	"module": "index.ts"
}
